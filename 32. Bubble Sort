//Bubble sort
#include<stdio.h>
int main()
{
    int arr[5]={10,21,3,2,45};
    int i,j,temp;
    for(i=0;i<5;i++)
    {
        for(j=0;j<5;j++)
        {
            if(arr[j]>arr[j+1])
            {
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
    printf("Sorted array is : ");
    for(i=0;i<5;i++)
    printf("%d ",arr[i]);
    
    return 0;
}
